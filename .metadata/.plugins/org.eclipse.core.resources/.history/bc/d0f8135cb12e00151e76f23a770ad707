package com.example.andenginetest;

import java.util.ArrayList;
import java.util.Collections;

public class Suffle  {
	
	public static ArrayList getSuffledCards(){
		
		
		ArrayList <Integer> list = new ArrayList<Integer>();
		for (int i=0; i <52;i++) {
			list.add(new Integer(i));
		}
		
		list =shuffle(list);
		
		
		return list;
	}

	
	public static  ArrayList<Integer> shuffle (ArrayList<Integer> list ) {
		
		ArrayList<Integer> shuffled,unshuffled;
		
		shuffled = list ;
		
		
		
		return list;
		
	}
	
	
	
}
